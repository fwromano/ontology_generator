@prefix drone: <http://example.org/drone#> .
@prefix env: <http://example.org/env#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sensor: <http://example.org/sensor#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

drone:DroneX a drone:Quadcopter ;
    rdfs:label "DroneX" ;
    drone:hasComponent drone:BatteryX,
        drone:FlightControllerX ;
    drone:hasSensor sensor:Camera1 .

drone:hasComponent a owl:ObjectProperty ;
    rdfs:label "hasComponent" ;
    rdfs:domain drone:Drone ;
    rdfs:range drone:DroneComponent .

drone:hasSensor a owl:ObjectProperty ;
    rdfs:label "hasSensor" ;
    rdfs:domain drone:Drone ;
    rdfs:range sensor:Sensor .

env:LocationA a env:Environment ;
    rdfs:label "LocationA" ;
    env:temperature 25.0 .

env:TreeA a env:Obstacle ;
    rdfs:label "TreeA" .

env:WeatherCondition a owl:Class ;
    rdfs:label "WeatherCondition" ;
    rdfs:subClassOf env:Environment .

env:hasObstacle a owl:ObjectProperty ;
    rdfs:label "hasObstacle" ;
    rdfs:domain env:Environment ;
    rdfs:range env:Obstacle .

env:temperature a owl:DatatypeProperty ;
    rdfs:label "temperature" ;
    rdfs:domain env:Environment ;
    rdfs:range xsd:decimal .

sensor:LidarSensor a owl:Class ;
    rdfs:label "LidarSensor" ;
    rdfs:subClassOf sensor:Sensor .

sensor:hasRange a owl:DatatypeProperty ;
    rdfs:label "hasRange" ;
    rdfs:domain sensor:Sensor ;
    rdfs:range xsd:integer .

drone:Battery a owl:Class ;
    rdfs:label "Battery" ;
    rdfs:subClassOf drone:DroneComponent .

drone:BatteryX a drone:Battery ;
    rdfs:label "BatteryX" .

drone:FlightController a owl:Class ;
    rdfs:label "FlightController" ;
    rdfs:subClassOf drone:DroneComponent .

drone:FlightControllerX a drone:FlightController ;
    rdfs:label "FlightControllerX" .

drone:Quadcopter a owl:Class ;
    rdfs:label "Quadcopter" ;
    rdfs:subClassOf drone:Drone .

sensor:Camera1 a sensor:CameraSensor ;
    rdfs:label "Camera1" ;
    sensor:hasRange 100 .

sensor:CameraSensor a owl:Class ;
    rdfs:label "CameraSensor" ;
    rdfs:subClassOf sensor:Sensor .

env:Obstacle a owl:Class ;
    rdfs:label "Obstacle" ;
    rdfs:subClassOf env:Environment .

drone:Drone a owl:Class ;
    rdfs:label "Drone" .

drone:DroneComponent a owl:Class ;
    rdfs:label "DroneComponent" .

sensor:Sensor a owl:Class ;
    rdfs:label "Sensor" .

env:Environment a owl:Class ;
    rdfs:label "Environment" .

