@prefix env: <http://example.org/env#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########## Classes ##########
env:Environment a owl:Class ;
    rdfs:label "Environment" .

env:WeatherCondition a owl:Class ;
    rdfs:subClassOf env:Environment ;
    rdfs:label "WeatherCondition" .

env:Obstacle a owl:Class ;
    rdfs:subClassOf env:Environment ;
    rdfs:label "Obstacle" .

########## Object Properties ##########
env:hasObstacle a owl:ObjectProperty ;
    rdfs:domain env:Environment ;
    rdfs:range env:Obstacle ;
    rdfs:label "hasObstacle" .

########## Data Properties ##########
env:temperature a owl:DatatypeProperty ;
    rdfs:domain env:Environment ;
    rdfs:range xsd:decimal ;
    rdfs:label "temperature" .

########## Individuals ##########
env:LocationA a env:Environment ;
    rdfs:label "LocationA" ;
    env:temperature 25.0 .

env:TreeA a env:Obstacle ;
    rdfs:label "TreeA" .
