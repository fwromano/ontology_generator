@prefix : <http://example.org/drone#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########## Classes ##########
:Drone a owl:Class ;
    rdfs:label "Drone" .

:Quadcopter a owl:Class ;
    rdfs:subClassOf :Drone ;
    rdfs:label "Quadcopter" .

:DroneComponent a owl:Class ;
    rdfs:label "Drone Component" .

:Battery a owl:Class ;
    rdfs:subClassOf :DroneComponent ;
    rdfs:label "Battery" .

:FlightController a owl:Class ;
    rdfs:subClassOf :DroneComponent ;
    rdfs:label "Flight Controller" .

:Sensor a owl:Class ;
    rdfs:label "Generic Sensor" .

:CameraSensor a owl:Class ;
    rdfs:subClassOf :Sensor ;
    rdfs:label "Camera Sensor" .

:LidarSensor a owl:Class ;
    rdfs:subClassOf :Sensor ;
    rdfs:label "Lidar Sensor" .

:Environment a owl:Class ;
    rdfs:label "Environment" .

:Obstacle a owl:Class ;
    rdfs:subClassOf :Environment ;
    rdfs:label "Obstacle" .

:WeatherCondition a owl:Class ;
    rdfs:subClassOf :Environment ;
    rdfs:label "Weather Condition" .

########## Object Properties ##########
:hasSensor a owl:ObjectProperty ;
    rdfs:domain :Drone ;
    rdfs:range :Sensor ;
    rdfs:label "has Sensor" .

:hasComponent a owl:ObjectProperty ;
    rdfs:domain :Drone ;
    rdfs:range :DroneComponent ;
    rdfs:label "has Component" .

:operatesIn a owl:ObjectProperty ;
    rdfs:domain :Drone ;
    rdfs:range :Environment ;
    rdfs:label "operates in" .

:hasObstacle a owl:ObjectProperty ;
    rdfs:domain :Environment ;
    rdfs:range :Obstacle ;
    rdfs:label "has Obstacle" .

########## Individuals (Instances) ##########
:DroneX a :Quadcopter ;                 # DroneX is a Quadcopter, which is a subclass of Drone
    rdfs:label "DroneX" ;
    :hasSensor :CameraA, :LidarA ;
    :hasComponent :BatteryA, :FlightCtrlA ;
    :operatesIn :TestArena .

:CameraA a :CameraSensor ;
    rdfs:label "CameraA" .

:LidarA a :LidarSensor ;
    rdfs:label "LidarA" .

:BatteryA a :Battery ;
    rdfs:label "BatteryA" .

:FlightCtrlA a :FlightController ;
    rdfs:label "Flight Controller A" .

:TestArena a :Environment ;
    rdfs:label "Test Arena" ;
    :hasObstacle :WallObstacle .

:WallObstacle a :Obstacle ;
    rdfs:label "Wall Obstacle" .
