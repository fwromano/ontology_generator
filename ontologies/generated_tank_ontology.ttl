@prefix : <http://example.org/mydomain#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
:Tank a owl:Class ;
    rdfs:label "Tank" .

:CrewPosition a owl:Class ;
    rdfs:label "Crew Position" .

:TankCommander a owl:Class ;
    rdfs:subClassOf :CrewPosition ;
    rdfs:label "Tank Commander" .

:Gunner a owl:Class ;
    rdfs:subClassOf :CrewPosition ;
    rdfs:label "Gunner" .

:Driver a owl:Class ;
    rdfs:subClassOf :CrewPosition ;
    rdfs:label "Driver" .

:Loader a owl:Class ;
    rdfs:subClassOf :CrewPosition ;
    rdfs:label "Loader" .

:Document a owl:Class ;
    rdfs:label "Document" .

:ProcedureGuide a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Procedure Guide" .

:TechnicalManual a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Technical Manual" .

:Task a owl:Class ;
    rdfs:label "Task" .

:CheckProcedure a owl:Class ;
    rdfs:label "Check Procedure" .

:BeforeOperationsPMCS a owl:Class ;
    rdfs:subClassOf :CheckProcedure ;
    rdfs:label "Before Operations PMCS" .

:DuringOperationsPMCS a owl:Class ;
    rdfs:subClassOf :CheckProcedure ;
    rdfs:label "During Operations PMCS" .

:AfterOperationsPMCS a owl:Class ;
    rdfs:subClassOf :CheckProcedure ;
    rdfs:label "After Operations PMCS" .

:Equipment a owl:Class ;
    rdfs:label "Equipment" .

:MainGun a owl:Class ;
    rdfs:subClassOf :Equipment ;
    rdfs:label "Main Gun" .

:CoaxialMachineGun a owl:Class ;
    rdfs:subClassOf :Equipment ;
    rdfs:label "Coaxial Machinegun" .

:GasParticulateFilter a owl:Class ;
    rdfs:subClassOf :Equipment ;
    rdfs:label "Gas Particulate Filter" .

# Object Properties
:performsTask a owl:ObjectProperty ;
    rdfs:domain :CrewPosition ;
    rdfs:range :Task ;
    rdfs:label "performs task" .

:hasTask a owl:ObjectProperty ;
    rdfs:domain :CrewPosition ;
    rdfs:range :Task ;
    rdfs:label "has task" .

:describedIn a owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :Document ;
    rdfs:label "described in" .

:usesEquipment a owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :Equipment ;
    rdfs:label "uses equipment" .

:hasPart a owl:ObjectProperty ;
    rdfs:domain :Tank ;
    rdfs:range :Equipment ;
    rdfs:label "has part" .

:performsCheck a owl:ObjectProperty ;
    rdfs:domain :CrewPosition ;
    rdfs:range :CheckProcedure ;
    rdfs:label "performs check" .

:describesFor a owl:ObjectProperty ;
    rdfs:domain :Document ;
    rdfs:range :CrewPosition ;
    rdfs:label "describes procedures for" .

# Individuals (Examples)
:M1AbramsTank a :Tank ;
    rdfs:label "M1 Abrams Tank" ;
    :hasPart :MainGun_M1, :CoaxialMachineGun_Cal50, :GasParticulateFilter_M240 .

:TankCommanderGuide a :ProcedureGuide ;
    rdfs:label "Tank Commander Procedure Guide" ;
    :describesFor :TankCommander .

:PrepareStation a :Task ;
    rdfs:label "Prepare Station" ;
    :describedIn :TankCommanderGuide ;
    :usesEquipment :GasParticulateFilter_M240 .

:PowerUpStation a :Task ;
    rdfs:label "Power Up Station/Turret" ;
    :describedIn :TankCommanderGuide ;
    :usesEquipment :MainGun_M1 .

:BeforeOpsCheck a :BeforeOperationsPMCS ;
    rdfs:label "Before Operations PMCS" .

:MainGun_M1 a :MainGun ;
    rdfs:label "M1 Main Gun" .

:CoaxialMachineGun_Cal50 a :CoaxialMachineGun ;
    rdfs:label "Cal .50 Coaxial Machinegun" .

:GasParticulateFilter_M240 a :GasParticulateFilter ;
    rdfs:label "M240 Gas Particulate Filter" .

# Crew Position Relationships
:TankCommander :hasTask :PrepareStation, :PowerUpStation ;
    :performsCheck :BeforeOpsCheck .
